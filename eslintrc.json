{
    "extends": ["eslint:recommended", 
      "plugins:import/errors",
      "plugins:react/recommended",
      "plugins:jsx-ally/recommended",
      "prettier", 
      "prettier/react"],
      "rules":{
        "react/prop-types":0,
        "no-console" : 1,
        "react-hooks/rules-of-hooks":2,
        "react-hooks/exhaustive-deps": 1
        
      },
    "plugins": ["react","import","jsx-ally","react-hooks"],
    "parser" : "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "es6": true,
      "browser": true,
      "node": true
    },
    "settings":{
      "react":{
        "version": "detect"

      }
    }
  }