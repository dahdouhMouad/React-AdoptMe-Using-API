s","original":{"line":76,"column":8}},{"name":"props","generated":{"line":94,"column":132},"source":"Details.js","original":{"line":76,"column":21}},{"generated":{"line":94,"column":137},"source":"Details.js","original":{"line":76,"column":8}},{"generated":{"line":94,"column":138},"source":"Details.js","original":{"line":75,"column":4}},{"generated":{"line":94,"column":139},"source":"Details.js","original":{"line":74,"column":4}},{"generated":{"line":95,"column":0},"source":"Details.js","original":{"line":79,"column":3}},{"generated":{"line":95,"column":2},"source":"Details.js","original":{"line":82,"column":0}},{"generated":{"line":96,"column":0},"source":"Details.js","original":{"line":83,"column":0}}],"sources":{"Details.js":"import React from \"react\";\r\nimport pet from \"@frontendmasters/pet\";\r\nimport {navigate} from '@reach/router'\r\nimport Modal from './Modal'\r\nimport Carousel from \"./Carousel\" ;\r\nimport ErrorBoundary from \"./ErrorBoundary\"\r\n\r\nclass Details extends React.Component{\r\n    //state= { loading :true} valid because we configure babel with eslint\r\n    constructor() {\r\n        super();\r\n        this.state = { loading: true,showModal:false };\r\n      }\r\n    //componentDidMount do one time then leave example ajax request  \r\n    componentDidMount(){\r\n        //pass it the id of the animals in the url \r\n        // this.props: can only read what inside can not change it\r\n        pet.animal(this.props.id).then(({animal}) => {\r\n            this.setState({\r\n                url: animal.url,\r\n                name: animal.name,\r\n                animal: animal.type,\r\n                location: `${animal.contact.address.city}, ${\r\n                  animal.contact.address.state\r\n                }`,\r\n                description: animal.description,\r\n                media: animal.photos,\r\n                breed: animal.breeds.primary,\r\n                loading: false,\r\n                \r\n                \r\n              });\r\n        }, console.error);\r\n\r\n    }\r\n\r\n    toggleModal = () => this.setState({showModal : !this.state.showModal});\r\n    adopt =() => navigate(this.state.url);\r\n    //class in react must have render\r\n    render(){\r\n        if(this.state.loading){\r\n            return <h1>Loading..</h1>;\r\n        }\r\n\r\n        const {name,animal,location,description,media,breed,showModal} = this.state;\r\n\r\n        return(\r\n            <div className=\"details\">\r\n                <Carousel media ={media} />\r\n                <div>\r\n                <h1>{name}</h1>\r\n                <h2>{`${animal} - ${breed} - ${location}`}</h2>\r\n                <button onClick={this.toggleModal}>Adopt {name}</button>\r\n                <p>{description}</p>\r\n                {showModal ? (\r\n                    <Modal>\r\n                        <div>\r\n                        <h1>Would Like To adopt {name}?</h1>\r\n                         <div className=\"buttons\">\r\n                            <button onClick={this.adopt}>Yes</button>\r\n                            <button onClick={this.toggleModal}>No</button>\r\n                         </div>\r\n                        </div>\r\n                    </Modal>\r\n\r\n                ):null};\r\n\r\n                </div>\r\n            </div>\r\n        );\r\n    }\r\n}\r\nexport default function DetailsWithErrorBoundary(props){\r\n    return(\r\n    <ErrorBoundary>\r\n        <Details {...props}/>\r\n    </ErrorBoundary>\r\n\r\n) }\r\n\r\n\r\n// <Details {...props}/> Solution Stack OverFlow\r\n// we can use this.props = props in render \r\n"},"lineCount":null}},"error":null,"hash":"f69d2a97f4e58bd793f9b860e76936cc","cacheData":{"env":{}}}