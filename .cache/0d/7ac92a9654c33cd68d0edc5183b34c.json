rated":{"line":80,"column":4},"source":"SearchParams.js","original":{"line":70,"column":21}},{"generated":{"line":80,"column":8},"source":"SearchParams.js","original":{"line":70,"column":25}},{"name":"pets","generated":{"line":80,"column":10},"source":"SearchParams.js","original":{"line":70,"column":28}},{"generated":{"line":81,"column":0},"source":"SearchParams.js","original":{"line":70,"column":12}},{"generated":{"line":81,"column":4},"source":"SearchParams.js","original":{"line":51,"column":8}},{"generated":{"line":81,"column":5},"source":"SearchParams.js","original":{"line":50,"column":4}},{"generated":{"line":82,"column":0},"source":"SearchParams.js","original":{"line":74,"column":1}},{"generated":{"line":82,"column":1},"source":"SearchParams.js","original":{"line":9,"column":0}},{"name":"SearchParams","generated":{"line":84,"column":15},"source":"SearchParams.js","original":{"line":75,"column":15}}],"sources":{"SearchParams.js":"import React,{useState,useEffect} from \"react\";\r\nimport pet,{ANIMALS} from '@frontendmasters/pet';//Parsel Install for us\r\n\r\nimport useDropdown from './useDropdown';\r\nimport Results from './Results';\r\n\r\n\r\n\r\nconst SearchParams = () =>{\r\n    //// Declare a new state variable, which we'll call \"setLocation\"\r\n    const [location,setLocation] = useState(\"California,CA\")// California default\r\n    // ANIMALS API : map Liste string To <options>... \r\n    // using Key have to be unique\r\n    //const [animal,setAnimal] = useState(\"dog\")\r\n    //const[breed,setBreed] = useState(\"\")\r\n    const[breeds,setBreeds] = useState([]);\r\n    const [animal,AnimalDropdown] = useDropdown(\"Animal\",\"dog\",ANIMALS);\r\n    const [breed, BreedDropdown,setBreed] = useDropdown(\"Breed\",\"\",breeds)\r\n    const [pets, setPets] = useState([])// empty fisrt then add with API\r\n\r\n    //async functions\r\n    async function requestPets(){\r\n        const {animals} = await pet.animals({\r\n            location,\r\n            breed,\r\n            type : animal\r\n\r\n\r\n        });\r\n        console.log(\"animals\",animals)\r\n        setPets(animals || []);\r\n\r\n\r\n    }\r\n\r\n    useEffect(()=>{\r\n        //pet.breeds(\"dog\").then(console.log)\r\n        setBreeds([]);\r\n        setBreed(\"\");//Clear\r\n\r\n        //retrieve from api\r\n        pet.breeds(animal).then(({ breeds : apiBreeds }) => {\r\n            const breedString = apiBreeds.map(({name})=>name);\r\n            setBreeds(breedString);\r\n\r\n        },error =>console.error(error));\r\n    },//when effect apply answer when this thing change\r\n    [animal,setBreed,setBreeds]);\r\n\r\n    return(\r\n        <div className=\"search-params\">\r\n            <form onSubmit = {e => {\r\n                e.preventDefault();\r\n                requestPets();\r\n            }}>\r\n                <label htmlFor=\"location\">\r\n                    <input id=\"location\" \r\n                     value={location}\r\n                     placeholder=\"Location\"\r\n                     onChange= {e => setLocation(e.target.value)\r\n                     }\r\n                     />\r\n                </label>\r\n                <AnimalDropdown />\r\n                <BreedDropdown />\r\n\r\n              \r\n                <button>Sumbit</button>\r\n            </form>\r\n            <Results pets ={pets}/>\r\n\r\n        </div>\r\n    );\r\n};\r\nexport default SearchParams;"},"lineCount":null}},"error":null,"hash":"f882ff1e4040bd87df0f2404c0ec6bb9","cacheData":{"env":{}}}